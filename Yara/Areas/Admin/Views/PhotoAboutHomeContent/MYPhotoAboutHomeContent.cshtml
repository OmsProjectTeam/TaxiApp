@model ViewmMODeElMASTER
@{
    ViewData["Title"] = ResourceWeb.LBMYPhotoAboutHomeContent;
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="col-lg-12 col-xs-12">
    <div class="box-content">
        <ul class="list-inline">
            <li class="margin-bottom-10"><a href="#"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">@ResourceWeb.LBMYPhotoAboutHomeContent</button></a></li>/
            <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="Home" asp-action="Index"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">لوحة التحكم</button></a></li>/
            <li class="margin-bottom-10"><a asp-area="" asp-controller="Home" asp-action="Index"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">الرئيسية</button></a></li>
        </ul>
        <!-- /.box-title -->
        <div class="dropdown js__drop_down">
            <a asp-area="Admin" asp-controller="PhotoAboutHomeContent" asp-action="AddEditPhotoAboutHomeContent"><button type="button" class="btn btn-violet btn-rounded btn-bordered waves-effect waves-light" style="margin-bottom:20px">Add</button></a>
        </div>
        <!-- /.dropdown js__dropdown -->
        <div id="example_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div id="rootwizard-progress">
                        <div class="tab-header">
                            <div class="navbar">
                                <div class="navbar-inner">
                                    <ul>
                                        @*  <li><a href="#tab-progress-1" data-toggle="tab">@ResourceWeb.LBInformaitionAdditionalPowers</a></li> *@
                                        @*  <li><a href="#tab-progress-2" data-toggle="tab">@ResourceWeb.LBAddressestelephones</a></li> *@
                                        @*   <li><a href="#tab-progress-4" data-toggle="tab">@ResourceWeb.GeneralManagerInformation</a></li> *@
                                        <li><a href="#tab-progress-3" data-toggle="tab">@ResourceWeb.LBMYPhotoAboutHomeContent</a></li>
                                        @* <li><a href="#tab-progress-5" data-toggle="tab">Fifth</a></li> *@
                                    </ul>
                                </div>
                                <div class="navbar-inner" style="margin-top:8px">
                                    <span style="color:royalblue"> <strong>@TempData["Saved successfully"]</strong></span>
                                    <span style="color:red"> <strong>@TempData["ErrorSave"]</strong></span>
                                </div>
                            </div>
                        </div>
                        <div class="tab-content">
                            <div id="bar" class="progress">
                                <div class="progress-bar active progress-bar-info progress-bar-striped bar"></div>
                            </div>
                            <div class="tab-pane" id="tab-progress-3">
                                <div class='table-responsive'>
                                    <div style="overflow-x:auto;">
                                        <table id="example2" class="table table-striped table-bordered display dataTable table-responsive" style="width: 100%;" role="grid" aria-describedby="example_info">
                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting_asc" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-sort="ascending">@ResourceWeb.LbId</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">@ResourceWeb.LbPhoto</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">@ResourceWeb.LbDataEntry</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">@ResourceWeb.LbDateTimeEntry</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">@ResourceWeb.lbCurrentState</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">@ResourceWeb.LbOpration</th>
                                            </thead>
                                            <tfoot>
                                                <tr>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbId</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbPhoto</th>
                                                  
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbDataEntry</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbDateTimeEntry</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.lbCurrentState</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbOpration</th>
                                            </tfoot>
                                            <tbody>
                                                @foreach (var item in Model.ListPhotoAboutHomeContent)
                                                {
                                                    <tr role="row" class="odd">
                                                        <td>@item.IdPhotoAboutHomeContent</td>
                                                        <td class="sorting_1"> <img src="@string.Format("/Images/Home/{0}",item.Photo)" class="img-bordered img-circle img-circle" style="width:100px; height:100px"></td>

                                                        <td>@item.DataEntry</td>
                                                        <td>@item.DateTimeEntry</td>
                                                        <td><div class="switch primary"><input type="checkbox" checked="@item.CurrentState" IdPhotoAboutHomeContent="switch-2" disabled><label for="switch-2">@item.CurrentState</label></div></td>
                                                        <td>
                                                            <ul class="list-inline">
                                                                <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="PhotoAboutHomeContent" asp-action="AddEditPhotoAboutHomeContentImage" asp-route-IdPhotoAboutHomeContent="@item.IdPhotoAboutHomeContent"><button type="button" class="btn btn-primary btn-circle btn-xs waves-effect waves-light"><i class="ico fa fa-edit"></i></button></a></li>|
                                                                <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="PhotoAboutHomeContent" asp-action="DeleteData" asp-route-IdPhotoAboutHomeContent="@item.IdPhotoAboutHomeContent"><button type="button" class="btn btn-danger btn-circle btn-xs waves-effect waves-light"><i class="ico fa fa-remove"></i></button></a></li>
                                                            </ul>
                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <ul class="pager wizard">
                                <li class="previous"><a href="javascript:void(0)">Previous</a></li>
                                <li class="next"><a href="javascript:void(0)">Next</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- /.box-content -->
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 col-xs-12">
                <div class="dataTables_info" id="example_info" role="status" aria-live="polite">Showing 1 to 4 of 4 entries (filtered from 57 total entries)</div>
            </div><div class="col-sm-7"><div class="dataTables_paginate paging_simple_numbers" id="example_paginate"><ul class="pagination"><li class="paginate_button previous disabled" id="example_previous"><a href="#" aria-controls="example" data-dt-idx="0" tabindex="0">Previous</a></li><li class="paginate_button active"><a href="#" aria-controls="example" data-dt-idx="1" tabindex="0">1</a></li><li class="paginate_button next disabled" id="example_next"><a href="#" aria-controls="example" data-dt-idx="2" tabindex="0">Next</a></li></ul></div></div>
        </div>
    </div>
</div>
<!-- /.box-content -->
@* <img src="~/images/user gide/PhotoAboutHomeContent.png" width="1920" height="1077" /> *@

@section Scripts
{
    <!-- Data Tables -->
    <script src="~/Admin/assets/plugin/datatables/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Admin/assets/plugin/datatables/media/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Admin/assets/plugin/datatables/extensions/Responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Admin/assets/plugin/datatables/extensions/Responsive/js/responsive.bootstrap.min.js"></script>
    <script src="~/Admin/assets/scripts/datatables.demo.min.js"></script>
    <script src="~/Admin/assets/plugin/form-wizard/prettify.js"></script>
    <script src="~/Admin/assets/plugin/form-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script src="~/Admin/assets/plugin/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Admin/assets/scripts/form.wizard.init.min.js"></script>
    <script src="~/js/site.js"></script>

}






